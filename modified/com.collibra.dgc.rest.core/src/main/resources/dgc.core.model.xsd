		<attribute name="resourceId" type="dgc:UUIDType" use="required" />
		<attribute name="name" type="string" use="required" />
		<attribute name="type" type="dgc:resourceTypeType" use="required" />
		<attribute name="score" type="double" use="optional" />
		<attribute name="attribute" type="string" use="optional" />
	</complexType>
	
