   <process id="BSG_Task_Management" isExecutable="true" name="BSG_Task_Management">
      <startEvent id="StartNoneEvent" name="StartNoneEvent" activiti:formKey="TasksCode.NewWorkflowTask"/>
      
      <userTask id="Perform" name="Perform" >
      		<humanPerformer>
				<resourceAssignmentExpression>
					<formalExpression>#{assignee}</formalExpression>
				</resourceAssignmentExpression>
			</humanPerformer>			 
      </userTask>
      
      <exclusiveGateway gatewayDirection="Diverging" id="state____closed__" name="state = 'closed'?" />

      <endEvent id="EndNoneEvent" name="EndNoneEvent" />
      
      <sequenceFlow id="SequenceFlow" name="SequenceFlow" sourceRef="StartNoneEvent" targetRef="Perform" />
      <sequenceFlow id="No" name="No" sourceRef="state____closed__" targetRef="Perform" >
      		<conditionExpression id="expressionProcessNotEnded" xsi:type="tFormalExpression">${!closed}</conditionExpression>
      </sequenceFlow>
      <sequenceFlow id="SequenceFlow_2" name="SequenceFlow" sourceRef="Perform" targetRef="state____closed__" />
      <sequenceFlow id="Yes" name="Yes" sourceRef="state____closed__" targetRef="EndNoneEvent" >
      		<conditionExpression id="expressionProcessEnded" xsi:type="tFormalExpression">${closed}</conditionExpression>
      </sequenceFlow>
   </process>
   
