					<element name="value" type="string" minOccurs="0" maxOccurs="unbounded" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="searchResultItemType">
		<sequence>
			<element name="parent" type="dgc:searchResultItemType"
