<?xml version="1.0" encoding="UTF-8"?>
<page-definition name="Glossary Vocabulary" description="The default glossary vocabulary page definition.">
  <configuration>
    <module name="pages/shared/page">
      <regions>
        <content>
          <module name="modules/layout/verticallayout">
            <rows>
              <row id="row1" />
              <row id="row2" />
              <row id="row3" />
            </rows>
            <regions>
              <row1>
                <module name="pages/shared/navigation" />
              </row1>
              <row2>
                <module name="modules/content/titlebar">
                  <resource-type>vocabulary</resource-type>
                </module>
              </row2>
              <row3>
                <module name="modules/layout/tabbar">
                  <tabs>
                    <tab1 index="1" id="overview" domid="overviewtab" domclass="overviewclass" enable="true" enablesettings="true" defaultactive="true" />
                  </tabs>
                  <regions>
                    <overview>
                      <module name="pages/glossary/tabs/overview" />
                    </overview>
                  </regions>
                </module>
              </row3>
            </regions>
          </module>
        </content>
      </regions>
    </module>
  </configuration>
  <matching-rules>
    <rule path="vocabulary" />
  </matching-rules>
</page-definition>